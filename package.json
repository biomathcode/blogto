{
  "name": "blogto",
  "displayName": "BlogTo",
  "description": "post blog from vscode to medium, dev, hashnode",
  "version": "0.6.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "icon": "logo.png",
  "repository": {
    "url": "https://github.com/pratiksharm/blogto"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:blogto.editor",
    "onLanguage:blogto",
    "onCommand:blogto.helloWorld",
    "onCommand:blogto.getActiveText",
    "onCommand:blogto.openconfig",
    "onCommand:blogto.hashnode_getuser",
    "onCommand:blogto.medium_getuser",
    "onCommand:blogto.dev_getuser",
    "onCommand:blogto.user_info",
    "onView:blog-sidebar"
  ],
  "main": "./dist/extension.js",
  "publisher": "coolhead",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "blog-sidebar",
          "title": "BlogTo",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "blog-sidebar": [
        {
          "type": "webview",
          "id": "blog-sidebar",
          "name": "blogto",
          "icon": "media/logo.svg",
          "contextualTitle": "Blogto"
        }
      ]
    },
    "commands": [
      {
        "command": "blogto.getActiveText",
        "title": "getActiveText",
        "category": "blogto"
      },
      {
        "command": "blogto.user_info",
        "title": "get user info stored in the local session",
        "category": "blogto"
      },
      {
        "command": "blogto.openconfig",
        "title": "Config",
        "category": "blogto"
      },
      {
        "command": "blogto.hashnode_getuser",
        "title": "Get user from hashnode",
        "category": "blogto"
      },
      {
        "command": "blogto.dev_getuser",
        "title": "Get user from dev.to",
        "category": "blogto"
      },
      {
        "command": "blogto.medium_getuser",
        "title": "Get user from medium",
        "category": "blogto"
      },
      {
        "command": "blogto.helloWorld",
        "title": "hello world",
        "category": "blogto"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^0.8.4",
    "axios": "^0.24.0"
  }
}
